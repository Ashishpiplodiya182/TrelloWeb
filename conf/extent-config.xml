<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>
		<!-- title of the document -->
		<documentTitle>Trello Automation Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<!-- report name - displayed at top-nav -->

		<!-- <reportName>Test Automation Report</reportName> -->
		<reportName>
		
		 		 <![CDATA[
		  <img src="https://super-monitoring.com/blog/wp-content/uploads/2019/06/trello.png"/
          <img src="data:image/png;base64,"/>
           
          
        ]]>   
			

		</reportName>


		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>bottom</testViewChartLocation>

		<!-- custom javascript -->
		<scripts>
        <![CDATA[
            $(document).ready(function() {

            });
        ]]>
        
        <js>
		<![CDATA[
		var element=document.getElementsByClassName("left panel-name")[0];
		element.innerHTML='<font size="5">Smart Recruit POC</font>';
		element.style.fontSize = "18px";
		element.style.color = "#8bff00";
		]]>
		</js>
		</scripts>
		
		<scripts>
        <![CDATA[
            $(document).ready(function() {

            });
        ]]>
       
        <js>
        <![CDATA[
        var element=document.getElementsByClassName("left panel-name")[0];
        element.innerHTML='<font size="5">Trello Automation Report</font>';
        element.style.fontSize = "18px";
        element.style.color = "#8bff00";
        ]]>

        <![CDATA[
        var element=document.getElementsByClassName('view-summary')[0].childNodes[1];
        element.innerHTML='<font size="5">Automation Report Summary</font>';
        element.style.fontSize = "12px";
        element.style.color = "#8bff00";
        ]]>
        </js>
        </scripts>
		

		<!-- custom styles -->
		<styles>
        <![CDATA[
            .report-name { padding-left: 10px; } .report-name > img { float: 
      left;height: 90%;margin-left: 30px;margin-top: 2px;width: auto; }
        ]]>
		</styles>
		 
		
	</configuration>
</extentreports>